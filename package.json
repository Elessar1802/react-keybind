{
  "name": "react-keybind",
  "version": "0.9.4",
  "description": "Global keybindings for your React application",
  "scripts": {
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "build": "tsc && uglifyjs  dist/index.js --compress --mangle -o dist/index.js --source-map \"filename='dist/index.js.map'\"",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "main": "index.js",
  "types": "dist/index.d.ts",
  "author": "Unicorn Heart Club",
  "license": "MIT",
  "repository": "github:UnicornHeartClub/react-keybind",
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^14.6.4",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4"
  },
  "volta": {
    "node": "18.19.1"
  },
  "lint-staged": {
    "*.{jsx?,tsx?,json,md}": "prettier --write"
  }
}
